{% include 'head.tpl.j2' %}
<body class="tex2jax_ignore">
    <header>
            <brand><i class="fab fa-python"></i><a href='/'>Home</a></brand>
        <nav> / <a href='/' >p</a>  &nbsp;/&nbsp; 
                <div class="dropdown">
                <a href={{module}}>{{module}}</a>&nbsp;/&nbsp;
                    <div class="dropdown-content">
                            {% for link,name in parts[module] -%}
                                <a href="{{url(link)}}{{ext}}">{{link.path}}</a>
                            {% endfor %}
                     </div>
                </div>
                <div class="dropdown">
                <a href='/p/{{module}}/{{version}}/api/{{module}}{{ext}}'>{{version}}</a>&nbsp;/&nbsp;
                    <div class="dropdown-content">
                            {% for v in [version] -%}
                                <a href="/p/{{module}}/{{v}}/api/{{module}}{{ext}}">{{v}}</a>
                            {% endfor %}
                    </div>
                </div>

                <div class="dropdown">
                    <a href='/p/{{module}}/{{version}}/api/{{module}}{{ext}}'>{{current_type}}</a>&nbsp;/&nbsp;
                    <div class="dropdown-content">
                        <a href="/p/{{module}}/{{version}}/api/{{module}}{{ext}}">API</a>
                        <a href="/p/{{module}}/{{version}}/gallery">Gallery</a>
                        <a href="/p/{{module}}/{{version}}/examples">Examples</a>
                     </div>
                </div>

            {% for p,links in parts.items() %}
                {% if links %}
                    <div class="dropdown">
                        {% if p =='+' %}
                            <a class="dropbtn" >{{p}}</a>{% if not loop.last %}.{% endif %}
                        {% else %}
                            <a class="dropbtn" href='{{parts_links[p]}}{{ext}}'>{{p}}</a>{% if not loop.last %}.{% endif %}
                        {% endif %}
                        {% if not loop.first %}
                            <div class="dropdown-content">
                            {% for link,name in links -%}
                                <a href="{{url(link)}}{{ext}}">{{name}}</a>
                            {% endfor %}
                            </div>
                        {% endif %}
                    </div>
                {% endif -%}
            {% endfor -%}
        </nav>
    </header>
<div class='container'>
    <div class="sidenav">
    {% if not logo %}
        <i class="fab fa-python"></i>
        <a href="#">Project Logo ^</a>
    {% else %}
        <img src="/p/{{module}}/{{version}}/img/{{logo}}"/>
    {% endif %}
    <a href="#">Library: {{module}}</a>
    <a href="#">Version: {{version}}</a>
    <a href="#">Authors:</a>
    <a href="#">PyPI:</a>
    <a href="#">Source</a>
    <a href="#">Homepage</a>
    <a href="#">Fundings</a>
    <a href="#">Stars</a>
    <a href="#">Forks</a>
    <a href="#">Related</a>
    <hr/>
    <a>Narrative Docs</a>
    <hr/>
    {% block plus %}
    {% endblock %}
    </div>

    {% block api %}
    {% endblock api %}

</div>
</body>
</html>
